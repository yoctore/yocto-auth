{
  "routes" : [
    {
      "path"              : "/users",
      "alias"             : ["/user"],
      "model"             : "user",
      "excluded"   : ["put", "delete" , "patch"]
    },
    {
      "path"              : "/account-sync",
      "alias"             : [],
      "model"             : "AccountSync",
      "excluded"   : ["put"],
      "methods" : [
        {
          "method"   : "post",
          "path"     : "sync",
          "fn"     : "addAcountSync"
        }
      ]
    },
    {
      "path"                : "/users/:user_id",
      "alias"               : ["/user/:user_id"],
      "model"               : "user",
      "paramToRetrieve"     : ["user_id"],
      "excluded"     : ["post"]
    },
    {
      "path"              : "/availability",
      "alias"             : ["/availability"],
      "model"             : "availability",
      "excluded"   : ["put", "delete" , "patch"]
    },
    {
      "path"                : "/availabilitys/:availability_id",
      "alias"               : ["/availabilies/:availability_id"],
      "model"               : "availability",
      "param"     : ["availability_id"],
      "excluded"     : ["post"]
    },
    {
      "path"              : "/storebrand",
      "alias"             : ["/storebrand"],
      "model"             : "storeBrand",
      "excluded"   : ["put", "delete" , "patch"]
    },
    {
      "path"                : "/storebrand/:storebrand_id",
      "alias"               : ["/storebrand/:storebrand_id"],
      "model"               : "storeBrand",
      "param"     : ["storebrand_id"],
      "excluded"     : ["post"]
    }
  ]
}
